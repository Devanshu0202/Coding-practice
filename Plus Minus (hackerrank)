#include<bits/stdc++.h>
using namespace std;
int main()
{
  int a,a1[100],total;
  float pos=0.,neg=0.,zer=0.;
  cin>>a;
  total=a;
  for(int i=0;i<a;i++)
{
    cin>>a1[i];
    if(a1[i]>0)
    pos++;
    else if(a1[i]<0)
    neg++;
    else zer++;
    }
    cout<<pos/total<<endl;
    cout<<neg/total<<endl;
    cout<<zer/total<<endl;
    return 0;
}
